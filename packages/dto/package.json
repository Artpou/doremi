  "exports": {
    "./search.dto": {
      "types": "./dist/search.dto.d.ts",
      "import": "./dist/search.dto.mjs",
      "require": "./dist/search.dto.js"
    },
    "./auth.dto": {
      "types": "./dist/auth.dto.d.ts",
      "import": "./dist/auth.dto.mjs",
      "require": "./dist/auth.dto.js"
